<UrlArray>
    <rewrite name="index" template="index.html" page="index.aspx" inherit="CSharp.WebLD.Page.index">
        <item path="index/{0}.aspx" pattern="index/(\w+).aspx" querystring="action=$1" />
        <item path="index/{0}/{1}.aspx" pattern="index/(\w+)/(\w+).aspx" querystring="action=$1^lid=$2" />
        <item path="index/{0}/{1}/{2}.aspx" pattern="index/(\w+)/(\w+)/(\w+).aspx" querystring="action=$1^lid=$2^page=$3" />
    </rewrite>
    <rewrite name="Main" template="Main.html" page="Main.aspx" inherit="CSharp.WebLD.Page.Main">
        <item path="Main/{0}.aspx" pattern="Main/(\w+).aspx" querystring="action=$1" />
        <item path="Main/{0}/{1}.aspx" pattern="Main/(\w+)/(\w+).aspx" querystring="action=$1^lid=$2" />
        <item path="Main/{0}/{1}/{2}.aspx" pattern="Main/(\w+)/(\w+)/(\w+).aspx" querystring="action=$1^lid=$2^page=$3" />
    </rewrite>
</UrlArray>

<!--
    说明:
    <UrlArray> // 根节点
    
    <rewrite // 重定向模板对象节点
        name="index" // 模板对象标识名称
        template="index.html" // 模板文件名称
        page="index.aspx" // 生成后的模板文件名称
        inherit="CSharp.WebLD.Page.index" // 模板继承那个Page类,用于处理后台程序
    >
        <item // 对应模板的具体URL重写样式
            path="Main/{0}/{1}.aspx" // 重写样子,带参数
            pattern="Main/(\w+)/(\w+).aspx" // 暂不明
            querystring="action=$1^lid=$2" // 参数,传给后台的时候使用什么参数名
        />
-->